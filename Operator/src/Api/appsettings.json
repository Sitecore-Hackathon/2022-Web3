{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Operator": {
        "InstanceImage": "mcr.microsoft.com/windows/servercore/iis:windowsservercore-ltsc2019",
        "InstanceNamePattern": "sc_{0}",
        "TraefikEntrypoint": "web",
        "HostNamePattern": "{0}.sitecore.localhost",
        "HostNameTls": false,
        "DefaultMemoryMb": 1024,
        "DefaultCpu": 2
    },
    "DockerEngine": {
        "Url": "npipe://./pipe/docker_engine"
    }
}
